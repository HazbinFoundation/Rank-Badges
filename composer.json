{
    "name": "hazbin/rank-badges",
    "description": "Replaces the default circle badges with rank-style badges.",
    "keywords": [
        "flarum",
        "flarum-extension",
        "hazbin"
    ],
    "type": "flarum-extension",
    "license": "MIT",
    "require": {
        "flarum/core": "^1.2"
    },
    "support": {
        "issues": "https://github.com/hazbinfoundation/rank-badges/issues",
        "source": "https://github.com/hazbinfoundation/rank-badges"
    },
    "authors": [
        {
            "name": "Hazbin Foundation",
            "email": "contact@hazbin.org",
            "role": "Developer"
        },
        {
            "name": "Jake McCarroll",
            "email": "jake.mccarroll@hazbin.net",
            "role": "Developer"
        }

    ],
    "homepage": "https://www.hazbin.org",
    "extra": {
        "flarum-extension": {
            "title": "Rank Badges",
            "category": "feauture",
            "icon": {
                "name": "fas fa-id-badge",
                "color": "#fff",
                "backgroundColor": "#DE6546"
            }
        },
        "flarum-cli": {
            "modules": {
                "admin": false,
                "forum": true,
                "js": true,
                "jsCommon": false,
                "css": true,
                "locale": false,
                "gitConf": true,
                "githubActions": true,
                "prettier": true,
                "typescript": false,
                "bundlewatch": false,
                "backendTesting": false,
                "editorConfig": true,
                "styleci": true
            }
        }
    }
}
